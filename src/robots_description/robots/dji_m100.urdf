        <inertia ixx="0.000000000001" ixy="0.0" ixz="0.0" iyy="0.0000000000000001" iyz="0.0" izz="0.00000000000000001" />
      </inertial>
    </link>
    <joint name="rotor3_joint" type="revolute">
      <origin xyz="-0.229809 0.229809 0.2" rpy="0.0 0.0 0.0" />
