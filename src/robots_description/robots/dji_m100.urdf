<?xml version="1.0"?>


<robot name="dji_m100" >

  <!-- <link name="base_link"></link> -->
    <link name="quadrotor">
      <inertial>
        <mass value="2.355" />
        <inertia ixx="0.01152" ixy="0.0" ixz="0.0" iyy="0.01152" iyz="0.0" izz="0.0218" />
      </inertial>
      <visual>
        <origin xyz="0 0 0.105" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://kr5_description/meshes/dji_m100.dae"/>
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0.105" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://kr5_description/meshes/dji_m100.dae"/>
        </geometry>
      </collision>
    </link>
    <link name="rotor1">
